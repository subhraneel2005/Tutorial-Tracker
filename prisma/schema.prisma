generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id     String      @id@default(uuid())
  email  String      @unique
  name   String
  photo  String?
  tutorials Tutorial[]
}

model Tutorial{
  id     String      @id@default(uuid())
  title  String
  link   String 
  notes  String?
  done   Boolean     @default(false)
  date   DateTime    @default(now())
  updatedAt   DateTime @updatedAt
  userId String
  user   User        @relation(fields: [userId], references: [id])
}



